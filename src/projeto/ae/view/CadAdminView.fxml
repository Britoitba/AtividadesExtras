<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projeto.ae.controller.CadAdminController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0" GridPane.columnIndex="1">
         <children>
            <ImageView fitHeight="600.0" fitWidth="794.0" layoutX="6.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/branco.jpg" />
               </image>
            </ImageView>
            <Label layoutX="135.0" layoutY="237.0" text="Login:">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Label layoutX="125.0" layoutY="287.0" text="Senha:">
               <font>
                  <Font name="Arial" size="20.0" />
               </font></Label>
            <Label alignment="CENTER" prefHeight="65.0" prefWidth="800.0" text="Cadastrar administrador">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <TextField fx:id="txtLogin" layoutX="203.0" layoutY="236.0" prefHeight="25.0" prefWidth="439.0">
               <font>
                  <Font size="15.0" />
               </font></TextField>
            <Button fx:id="btnCadastrar" layoutX="660.0" layoutY="539.0" mnemonicParsing="false" onAction="#cadastrar" prefHeight="39.0" prefWidth="118.0" text="Cadastrar">
               <font>
                  <Font name="Arial" size="18.0" />
               </font></Button>
            <PasswordField fx:id="pwdSenha" layoutX="203.0" layoutY="283.0" onAction="#onEnter" prefHeight="25.0" prefWidth="234.0">
               <font>
                  <Font size="15.0" />
               </font></PasswordField>
         </children>
      </Pane>
   </children>
</GridPane>
